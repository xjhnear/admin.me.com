PROPER			= TN'EV				##	Sz"oveg minden szavának kezd"obet"ujét nagybet"usre cseréli.
REPLACE			= CSERE				##	A sz"ovegen belül karaktereket cserél.
REPLACEB		= CSERE2			##	A sz"ovegen belül karaktereket cserél.
